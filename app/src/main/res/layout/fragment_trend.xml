<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_trend"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.rudolphriding.giftfriend.app.trend.TrendFragment">

    <include
        layout="@layout/item_category"
        android:id="@+id/category_category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <include
        layout="@layout/item_category"
        android:id="@+id/category_function"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/category_category"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"/>



</RelativeLayout>
